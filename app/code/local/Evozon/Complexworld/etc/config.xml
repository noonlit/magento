<config>
    <modules>
        <Evozon_Complexworld>
            <version>0.1.0</version>
        </Evozon_Complexworld>
    </modules> 
    <frontend>
        <routers>
            <complexworld>
                <use>standard</use>
                <args>
                    <module>Evozon_Complexworld</module>
                    <frontName>complexworld</frontName>
                </args>            
            </complexworld>
        </routers>    
    </frontend>
    <global>
        <models>
            <complexworld>
                <class>Evozon_Complexworld_Model</class>
                <resourceModel>complexworld_resource</resourceModel>
            </complexworld>
            <complexworld_resource>
                <class>Evozon_Complexworld_Model_Resource</class>
                <entities>
                    <eavblogpost>
                        <table>eavblog_posts</table>
                    </eavblogpost>                 
                </entities>
            </complexworld_resource>
        </models>
        <resources>
            <complexworld_setup>
                <setup>
                    <module>Evozon_Complexworld</module>
                    <class>Evozon_Complexworld_Model_Resource_Setup</class>
                </setup>
            </complexworld_setup>
        </resources>
    </global>
</config>